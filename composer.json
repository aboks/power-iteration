{
	"name": "aboks/power-iteration",
	"description": "Implementation of the Power Iteration method for finding eigenvalues of a matrix, using math-php",
	"minimum-stability": "stable",
	"license": "MIT",
	"keywords": ["math", "mathematics", "eigenvalue", "eigenvector", "linear-algebra", "math-php"],
	"authors": [
		{
			"name": "Arnout Boks",
			"email": "arnoutboks@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Aboks\\PowerIteration\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Aboks\\PowerIteration\\Tests\\": "test/"
		}
	},
	"require": {
		"php": "^7.3 || ^8.0",
		"markrogoyski/math-php": "^0.38.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5"
	}
}
